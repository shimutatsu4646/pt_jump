<h1>トップページ</h1>

<% @trainees.each do |trainee| %>
<ul class="custom-light-gray"> <li>名前:<%= trainee.name %></li>
  <li>年齢:<%= trainee.age %></li>
  <li>性別:<%= trainee.gender %></li>
  <li>メールアドレス:<%= trainee.email %></li>
  <li>DM許可:<%= trainee.dm_allowed %></li>
  <li><%= link_to 'プロフィール', trainee_path(trainee) %></li>
</ul>
<% end %>
